{
	"name": "curtistinkers/wordpress-plugin-template",
	"description": "",
	"type": "wordpress-plugin",
	"version": "1.0.1",
	"keywords": [ "wordpress", "composer", "wordpress-plugin" ],
	"homepage": "https://github.com/curtistinkers/wordpress-plugin-template",
	"license": "MIT",
	"authors": [
		{
			"name": "curtistinkers",
			"email": "curtistinkers.commit@gmail.com",
			"homepage": "https://curtistinkers.com",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/curtistinkers/wordpress-plugin-template/issues",
		"source": "https://github.com/curtistinkers/wordpress-plugin-template"
	},
	"require": {
		"php": ">=7.0"
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "^3.0",
		"phpstan/extension-installer": "^1.4",
		"phpstan/phpstan-strict-rules": "^2.0",
		"szepeviktor/phpstan-wordpress": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"Curtistinkers\\WordpressPluginTemplate\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"phpstan/extension-installer": true
		}
	},
	"scripts": {
		"lint" : "phpcs",
		"phpstan" : "phpstan analyse"
	}
}
